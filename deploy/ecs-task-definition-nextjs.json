{
  "family": "grpchat-web",
  "containerDefinitions": [
    {
      "name": "grpchat-web-container",
      "image": "413025517373.dkr.ecr.us-east-1.amazonaws.com/grpchat-web:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "grpchat-grpc-service",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::413025517373:role/GrpChatGRPCEcsExecutionRole"
}
